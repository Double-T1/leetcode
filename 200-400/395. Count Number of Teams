//O(n^2)
class Solution {
public:
    int numTeams(vector<int>& rating) {
        int n = rating.size();
        vector<int> biggerAhead(n,0);
        for (int i=0; i<n; i++) {
            for (int j=i+1; j<n; j++) {
                if (rating[i]<rating[j]) biggerAhead[i]++;
            }
        }
        
        int ans = 0;
        for (int i=0; i<n; i++) {
            for (int j=i+1; j<n; j++) {
                if (rating[j]>rating[i]) ans += biggerAhead[j];
                else ans += ((n-j-1)-biggerAhead[j]);
            }
        }
        return ans;
    }
};
