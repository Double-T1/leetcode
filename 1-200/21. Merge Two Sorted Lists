var mergeTwoLists = function(list1, list2) {
    let pseudoHead = new ListNode(), node = pseudoHead
    while (list1 || list2) {
        if (!list2 || (list1 && list1.val<list2.val)) {
            node.next = list1
            list1 = list1.next
        } else {
            node.next = list2
            list2 = list2.next
        }
        node = node.next
    }
    return pseudoHead.next
};
