class Solution {
public:
    ListNode* removeNthFromEnd(ListNode* head, int n) {
        ListNode* pseudo = new ListNode();
        pseudo->next = head;
        auto p1 = pseudo, p2=pseudo;
        //p2 will stop at he
        while (n) {
            n--;
            p1 = p1-> next;
        }
        while (p1->next) {
            p2 = p2->next;
            p1 = p1->next;
        }
        p2->next = p2->next->next;
        
        return pseudo->next;
    }
};
